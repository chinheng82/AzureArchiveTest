{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dvue2azuredatafactoryv2"
		},
		"ArchiveADLSGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ArchiveADLSGen2'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"ArchiveADLSGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dvue2azuredatalakev2.dfs.core.windows.net"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dvue2azuredatalakev2.dfs.core.windows.net"
		},
		"adlsue2pv_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/6d3d998c-abd9-4369-9f7a-9fc9a69ea4e4/resourceGroups/dvue2-adlsv2-rg/providers/Microsoft.Storage/storageAccounts/dvue2azuredatalakev2"
		},
		"adlsue2pv_properties_groupId": {
			"type": "string",
			"defaultValue": "dfs"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dvue2azuredatafactoryv2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ArchiveADLSGen2_accountKey": {
						"value": "[parameters('ArchiveADLSGen2_accountKey')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"adlsue2pv_properties_privateLinkResourceId": {
						"value": "[parameters('adlsue2pv_properties_privateLinkResourceId')]"
					},
					"adlsue2pv_properties_groupId": {
						"value": "[parameters('adlsue2pv_properties_groupId')]"
					},
					"ArchiveADLSGen2_properties_typeProperties_url": {
						"value": "[parameters('ArchiveADLSGen2_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}