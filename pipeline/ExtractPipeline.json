{
	"name": "ExtractPipeline",
	"properties": {
		"activities": [
			{
				"name": "ExtractFiles",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "mftadlsv2/ch/"
					},
					{
						"name": "Destination",
						"value": "archiveadlsv2/archive/"
					}
				],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"modifiedDatetimeEnd": {
								"value": "@string(formatDateTime(addminutes(utcnow(),-20),'yyyy-MM-ddThh:mm:ss.fffZ'))",
								"type": "Expression"
							},
							"deleteFilesAfterCompletion": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings",
							"compressionProperties": {
								"type": "TarGZipReadSettings",
								"preserveCompressionFileNameAsFolder": false
							}
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_f18",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_f18",
						"type": "DatasetReference",
						"parameters": {
							"peopath": {
								"value": "@concat('data','/','mft','/','suzjv','/','classtar','/','peo','/','incoming','/',formatDateTime(utcnow(),'yyyy'),formatDateTime(utcnow(),'MM'),formatDateTime(utcnow(),'dd'))",
								"type": "Expression"
							}
						}
					}
				]
			},
			{
				"name": "CopyDone",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "ExtractFiles",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"modifiedDatetimeEnd": {
								"value": "@string(formatDateTime(addminutes(utcnow(),-30),'yyyy-MM-ddThh:mm:ss.fffZ'))",
								"type": "Expression"
							},
							"deleteFilesAfterCompletion": true
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "TarSource",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "TarDestination",
						"type": "DatasetReference",
						"parameters": {
							"archivepath": "@concat('archive','/',\n'classtar','/','suzjv','/',formatDateTime(utcnow(),'yyyy'),formatDateTime(utcnow(),'MM'),formatDateTime(utcnow(),'dd'))"
						}
					}
				]
			}
		],
		"concurrency": 1,
		"annotations": []
	}
}